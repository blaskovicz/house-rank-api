    "express": "^4.16.3",
    "express-async-handler": "^1.1.4",
    "express-graphql": "^0.6.12",
    "google-auth-library": "^2.0.0",
    "graphql": "^14.0.2",
    "graphql-tools": "^4.0.0",
    "morgan": "^1.9.1",
    "node-zillow": "^2.0.0",
    "pg": "^7.5.0",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "uuid": "^3.3.2",
    "winston": "^3.1.0"
  },
