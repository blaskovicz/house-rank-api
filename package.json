    "@types/graphql": "^14.0.1",
    "@types/mocha": "^5.2.5",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.9.4",
    "@types/pg": "^7.4.10",
    "@types/redis": "^2.8.6",
    "@types/uuid": "^3.4.4",
    "concurrently": "^4.0.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-no-unused-expression-chai": "^0.1.3",
    "typescript": "^3.0.3"
  }
